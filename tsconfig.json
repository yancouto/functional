{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "outDir": "build",
    "rootDir": "src",
    "types": [
      "love-typescript-definitions",
      "lua-types/jit"
    ],
    "strict": true
  },
  "tstl": {
    "luaTarget": "JIT",
    "noImplicitSelf": true
  }
}